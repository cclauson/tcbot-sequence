{
  "name": "@cclauson/tcbot-sequence-demo-app",
  "version": "1.0.0",
  "description": "A simple Webpack 4 starter with TypeScript transpilation",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --profile --colors --watch --content-base src/ --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "build.esnext": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build.all": "npm run build.prod && npm run build.esnext",
    "test": "karma start karma.config.js"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript"
  ],
  "author": "Juri Strumpflohner",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jasmine": "2.8.7",
    "@types/node": "7.0.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/redux": "^3.6.0",
    "awesome-typescript-loader": "5.2.0",
    "jasmine-core": "3.1.0",
    "karma": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.0",
    "source-map-loader": "0.2.3",
    "ts-loader": "^9.2.6",
    "tslint": "5.17.0",
    "tslint-loader": "3.6.0",
    "typescript": "^3.9.10",
    "webpack": "^4.43.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.10.3"
  },
  "dependencies": {
    "@fluidframework/aqueduct": "^0.54.1",
    "@fluidframework/container-definitions": "^0.44.0",
    "@fluidframework/container-loader": "^0.54.1",
    "@fluidframework/local-driver": "^0.54.1",
    "@fluidframework/server-local-server": "^0.1034.0",
    "@fluidframework/view-interfaces": "^0.54.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
